                <div class="table-responsive mobileTable">
                    <table id="myTable" class="table table-sm table-bordered display w-100 mt-1 table-mobile-responsive table-mobile-sided">
                        <thead>
                            <tr>
                                @*  <th scope="col">Id</th>*@
                                <th class="text-nowrap" scope="col">Serial No.</th>
                                <th class="text-nowrap" scope="col">No. of Ticket</th>
                               @* <th scope="col">Draw Date</th>*@
                                <th scope="col">Draw Time</th>
                                @* <th scope="col">Draw No.</th>
                                <th scope="col">Ticket Code</th>*@
                                <th scope="col">Sam Type</th>
                                @*  <th scope="col">No. of available ticket</th>*@
                            </tr>
                        </thead>
                        @* <tbody id="ticketlist">
                        </tbody>*@
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row mt-2">
    <div class="col-12">
        <div class="card customCard">
            <div class="card-header">
                Total Users : <lable id="usercount"></lable>
            </div>
            <div class="card-body">
                <div class="table-responsive customTable">
                    <table id="tableDetails" class="table table-sm table-bordered display w-100 mt-1">
                        <thead>
                            <tr>
                                @*  <th scope="col">Id</th>*@
                                <th scope="col">Role</th>
                                <th scope="col">Action</th>
