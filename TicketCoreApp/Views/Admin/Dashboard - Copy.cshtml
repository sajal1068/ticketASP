<div class="modal fade" id="UserModal" aria-labelledby="exampleModalLabel" data-bs-backdrop="static" aria-hidden="true" tabindex="-1">
    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Add User</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12">
                        <form class="modalFormDesign">
                            <div class="mb-3">
                                <label for="userFirstName" class="form-label fw-bold">First Name</label>
                                <input type="text" class="form-control" id="userFirstName" maxlength="50" placeholder="Enter First Name" aria-describedby="emailHelp" />
                                <h6 id="checkFirstName" style="color: red;"> </h6>
                            </div>
                            <div class="mb-3">
                                <label for="userLastName" class="form-label fw-bold">Last Name</label>
                                <input type="text" class="form-control" id="userLastName" maxlength="50" placeholder="Enter Last Name" />
                                <h6 id="checkLastName" style="color: red;"> </h6>
                            </div>
                            <div class="mb-3">
                                <label for="userMobile" class="form-label fw-bold">Mobile Number</label>
                                <input type="text" class="form-control" id="userMobile" maxlength="10" placeholder="Enter Mobile Number" />
                                <h6 id="checkMobile" style="color: red;"> </h6>
                            </div>
                            <div class="mb-3">
                                <label for="userPassword" class="form-label fw-bold">Password</label>
                                <input type="password" class="form-control" id="userPassword" maxlength="15" placeholder="Enter Password" />
                                <h6 id="checkPassword" style="color: red;"> </h6>
                            </div>
                            <div class="mb-3">
                                <label for="userCity" class="form-label fw-bold">City</label>
                                <input type="text" class="form-control" id="userCity" maxlength="50" placeholder="Enter City" />
                                <h6 id="checkCity" style="color: red;"> </h6>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="btnAddUser" onclick="return AddUser();">Submit</button>
            </div>
        </div>
    </div>
</div>

<div class="modal" id="TicketModal" aria-labelledby="myModalLabel" data-bs-backdrop="static" data-bs-keyboard="false" aria-hidden="true" tabindex="-1">
    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Add Ticket</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="mb-3">
                        <label for="Name" class="form-label fw-bold">Draw Date</label>
                        <input type="text" class="form-control"  id="ticket_draw_date" placeholder="yyyy-mm-dd" />
                    </div>
                    <div class="mb-3">
                        <label for="Name" class="form-label fw-bold">Draw Time</label>
                        <select id="ticket_draw_time" class="form-control">
                        </select>
                    </div>
                    <div class="mb-3">
                        <label for="Age" class="form-label fw-bold">No. of Ticket</label>
                        <select id="no_of_ticket" class="form-control">
                            <option value="0">Select</option>
                            <option value="10">10</option>
                            <option value="20">20</option>
                            <option value="30">30</option>
                            <option value="50">50</option>
                            <option value="100">100</option>
                            <option value="200">200</option>
                            <option value="300">300</option>
                            <option value="400">400</option>
                            <option value="500">500</option>
                            <option value="1000">1000</option>
                        </select>
                    </div>
                    <div class="mb-3">
                        <label for="ticket_serial_no" class="form-label fw-bold">Ticket Serial No.</label>
                        <input type="text" class="form-control" id="ticket_serial_no" maxlength="5" placeholder="" />
                        <h6 id="checkTicketSerialNo" style="color: red;"> </h6>
                    </div>
                    <div class="mb-3">
                        <label for="ticket_draw_no" class="form-label fw-bold">Ticket Draw No.</label>
                        <input type="text" class="form-control" id="ticket_draw_no" maxlength="3" placeholder="" />
                        <h6 id="checkTicketDrawNo" style="color: red;"> </h6>
                    </div>
                    <div class="mb-3">
                        <label for="ticket_code" class="form-label fw-bold">Ticket Code</label>
                        <input type="text" class="form-control" id="ticket_code" maxlength="5" placeholder="" />
                        <h6 id="checkTicketCode" style="color: red;"> </h6>
                    </div>
                    <div class="mb-3">
                        <label for="ticket_sam_type" class="form-label fw-bold">Ticket Sam Type</label>
                        <select id="ticket_sam_type" class="form-control">
                        </select>
                    </div>
                </form>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="btnAddTicket" onclick="return AddTicket();">Submit</button>
            </div>

        </div>
    </div>
</div>

<div class="modal" id="AssignRoleModal" aria-labelledby="staticBackdropLabel" data-bs-backdrop="static" data-bs-keyboard="false" aria-hidden="true" tabindex="-1">
